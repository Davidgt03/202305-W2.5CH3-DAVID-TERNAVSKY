{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "test": true
  },
  "extends": ["xo", "eslint-config-prettier"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "overrides": [
    {
      "extends": ["xo-typescript"],
      "files": ["*.ts", "*.tsx"]
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },

  "rules": {}
}
